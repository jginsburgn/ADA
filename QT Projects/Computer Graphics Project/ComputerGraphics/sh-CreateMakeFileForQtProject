qmake CONFIG+=c++11 CONFIG+=debug CONFIG+=x86_64 -o Makefile ../ComputerGraphics/ComputerGraphics.pro
echo 'Completed. Now please just append: -I immediately followed by the directory containing Helper.h, to the Makefile line starting with INCPATH.'
